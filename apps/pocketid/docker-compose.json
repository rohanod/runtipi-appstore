{
  "services": [
    {
      "name": "pocket-id",
      "image": "ghcr.io/pocket-id/pocket-id:v0.53.0",
      "environment": {
        "PUBLIC_APP_URL": "${PUBLIC_APP_URL}"
      },
      "internalPort": 80,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data",
          "containerPath": "/app/backend/data",
          "readOnly": false,
          "shared": false,
          "private": false
        }
      ],
      "isMain": true
    }
  ]
}